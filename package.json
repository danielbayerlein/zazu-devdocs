{
  "name": "zazu-devdocs",
  "version": "1.0.0",
  "description": "Zazu plugin to search for API documentations with devdocs.io",
  "main": "src/index.js",
  "private": true,
  "scripts": {
    "lint": "eslint --format=node_modules/eslint-formatter-pretty .",
    "test": "npm run lint && jest",
    "prepush": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danielbayerlein/zazu-devdocs.git"
  },
  "keywords": [
    "zazu",
    "devdocs",
    "API",
    "documentation",
    "docs"
  ],
  "author": "Daniel Bayerlein",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/danielbayerlein/zazu-devdocs/issues"
  },
  "homepage": "https://github.com/danielbayerlein/zazu-devdocs#readme",
  "devDependencies": {
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-plugin-import": "^2.3.0",
    "husky": "^0.13.4",
    "jest": "^20.0.4"
  },
  "dependencies": {
    "got": "^7.0.0"
  },
  "jest": {
    "bail": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/src/*.js"
    ]
  }
}
